// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Kanto {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  pokeid    Int    @unique
  name      String
  abilities Json[]
  types     Json[]
}

model Teams {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  team  Json[]
  user  String
  title String
  date  String
}
